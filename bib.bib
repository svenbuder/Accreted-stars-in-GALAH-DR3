	keywords = {stars: variables: RR Lyrae, Galaxy: kinematics and dynamics, Galaxy: stellar content, Galaxy: structure, galaxies: individual (Milky Way), Astrophysics - Astrophysics of Galaxies},
	month = jan,
	number = {3},
	pages = {3868-3879},
