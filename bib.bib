	author = {{Reggiani}, Henrique and {Amarsi}, Anish M. and {Lind}, Karin and {Barklem}, Paul S. and {Zatsarinny}, Oleg and {Bartschat}, Klaus and {Fursa}, Dmitry V. and {Bray}, Igor and {Spina}, Lorenzo and {Mel{\'e}ndez}, Jorge},
	date-added = {2020-04-20 13:09:41 +1000},
	date-modified = {2020-04-20 13:09:46 +1000},
	doi = {10.1051/0004-6361/201935156},
	eid = {A177},
	eprint = {1906.08281},
