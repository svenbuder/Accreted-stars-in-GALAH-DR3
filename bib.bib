	journal = {\apjl},
	keywords = {Galaxy: halo, Galaxy: kinematics and dynamics, solar neighborhood, Astrophysics - Astrophysics of Galaxies},
	month = jun,
	number = {1},
	pages = {L11},
	primaryclass = {astro-ph.GA},
	title = {{One Large Blob and Many Streams Frosting the nearby Stellar Halo in Gaia DR2}},
	volume = {860},
	year = 2018,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.3847/2041-8213/aac882}}

@article{Koppelman2019,
	abstract = {{\textbackslash Aims: Several kinematic and chemical substructures have
        been recently found amongst Milky Way halo stars with retrograde
        motions. It is currently unclear how these various structures
        are related to each other. This Letter aims to shed light on
        this issue. \textbackslash Methods: We explore the retrograde
        halo with an augmented version of the Gaia DR2 RVS sample,
        extended with data from three large spectroscopic surveys,
        namely RAVE, APOGEE, and LAMOST. In this dataset, we identify
        several structures using the HDBSCAN clustering algorithm. We
        discuss their properties and possible links using all the
        available chemical and dynamical information. \textbackslash
        Results: In concordance with previous work, we find that stars
        with [Fe/H] $\lt$ -1 have more retrograde motions than those
        with [Fe/H] $\gt$ -1. The retrograde halo contains a mixture of
        debris from objects like Gaia-Enceladus, Sequoia, and even the
        chemically defined thick disc. We find that the Sequoia has a
        smaller range in orbital energies than previously suggested and
        is confined to high energy. Sequoia could be a small galaxy in
        itself, but since it overlaps both in integrals-of-motion space
        and chemical abundance space with the less bound debris of Gaia-
        Enceladus, its nature cannot yet be fully settled. In the low-
        energy part of the halo, we find evidence for at least one more
        distinct structure: Thamnos. Stars in Thamnos are on low-
        inclination, mildly eccentric retrograde orbits, moving at
        v$_{{\ensuremath{\phi}}}$ {\ensuremath{\approx}} -150 km
        s$^{-1}$, and are chemically distinct from the other structures.
        \textbackslash Conclusions: Even with the excellent Gaia DR2
        data, piecing together all the fragments found in the retrograde
        halo remains challenging. At this point, we are very much in
        need of large datasets with high-quality high-resolution spectra
        and tailored high-resolution hydrodynamical simulations of
        galaxy mergers.}},
	adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	adsurl = {https://ui.adsabs.harvard.edu/abs/2019A&A...631L...9K},
	archiveprefix = {arXiv},
	author = {{Koppelman}, Helmer H. and {Helmi}, Amina and {Massari}, Davide and {Price-Whelan}, Adrian M. and {Starkenburg}, Tjitske K.},
	date-added = {2020-08-13 13:57:39 +1000},
	date-modified = {2020-08-13 13:57:43 +1000},
	doi = {10.1051/0004-6361/201936738},
	eid = {L9},
	eprint = {1909.08924},
	journal = {\aap},
	keywords = {Galaxy: halo, solar neighborhood, Galaxy: kinematics and dynamics, Galaxy: formation, Galaxy: evolution, Astrophysics - Astrophysics of Galaxies},
	month = nov,
	pages = {L9},
	primaryclass = {astro-ph.GA},
	title = {{Multiple retrograde substructures in the Galactic halo: A shattered view of Galactic history}},
	volume = {631},
	year = 2019,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1051/0004-6361/201936738}}

@article{Vincenzo2019,
	abstract = {{We present the first chemical evolution model for Enceladus, alias the
        Gaia Sausage, to investigate the star formation history of one
        of the most massive satellites accreted by the Milky Way during
        a major merger event. Our best chemical evolution model for
        Enceladus nicely fits the observed stellar
        [{\ensuremath{\alpha}}/Fe]-[Fe/H] chemical abundance trends, and
        reproduces the observed stellar metallicity distribution
        function, by assuming low star formation efficiency, fast infall
        time-scale, and mild outflow intensity. We predict a median age
        for Enceladus stars 12.33\^\{+0.92\}\_\{-1.36\} Gyr, and - at
        the time of the merger with our Galaxy ({\ensuremath{\approx}}10
        Gyr ago from Helmi et al.) - we predict for Enceladus a total
        stellar mass M$_{{\ensuremath{\star}}}$ {\ensuremath{\approx}} 5
        {\texttimes} {}10$^{9}$ M$_{\textbackslashodot}$. By looking at
        the predictions of our best model, we discuss that merger events
        between the Galaxy and systems like Enceladus may have inhibited
        the gas accretion on to the Galaxy disc at high redshifts,
        heating up the gas in the halo. This scenario could explain the
        extended period of quenching in the star formation activity of
        our Galaxy about 10 Gyr ago, which is predicted by Milky Way
        chemical evolution models, in order to reproduce the observed
        bimodality in [{\ensuremath{\alpha}}/Fe]-[Fe/H] between thick-
        and thin-disc stars.}},
	adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	adsurl = {https://ui.adsabs.harvard.edu/abs/2019MNRAS.487L..47V},
	archiveprefix = {arXiv},
	author = {{Vincenzo}, Fiorenzo and {Spitoni}, Emanuele and {Calura}, Francesco and {Matteucci}, Francesca and {Silva Aguirre}, Victor and {Miglio}, Andrea and {Cescutti}, Gabriele},
	date-added = {2020-08-13 13:53:11 +1000},
	date-modified = {2020-08-13 13:53:17 +1000},
	doi = {10.1093/mnrasl/slz070},
	eprint = {1903.03465},
	journal = {\mnras},
	keywords = {stars: abundances, Galaxy: abundances, Galaxy: evolution, Galaxy: formation, galaxies: individual: Enceladus, galaxies: individual: Gaia Sausage, Astrophysics - Astrophysics of Galaxies},
	month = jul,
	number = {1},
	pages = {L47-L52},
	primaryclass = {astro-ph.GA},
	title = {{The Fall of a Giant. Chemical evolution of Enceladus, alias the Gaia Sausage}},
