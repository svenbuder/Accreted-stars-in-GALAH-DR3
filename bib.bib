	keywords = {planets and satellites: general, stars: abundances, stars: solar-type, Sun: abundances, techniques: spectroscopic, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	month = sep,
