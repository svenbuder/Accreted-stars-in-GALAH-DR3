	doi = {10.1088/0004-637X/722/2/1373},
	eprint = {1008.2232},
	journal = {\apj},
	keywords = {globular clusters: general, globular clusters: individual: Omega Centauri, stars: abundances, stars: Population II},
	month = oct,
	pages = {1373-1410},
	primaryclass = {astro-ph.SR},
	title = {{Chemical Abundances for 855 Giants in the Globular Cluster Omega Centauri (NGC 5139)}},
	volume = 722,
	year = 2010,
