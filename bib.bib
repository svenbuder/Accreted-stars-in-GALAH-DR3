	date-added = {2019-03-05 17:02:06 +1100},
	date-modified = {2019-03-05 17:02:06 +1100},
	doi = {10.1093/mnras/stw247},
	eprint = {1601.07354},
	journal = {\mnras},
	keywords = {planets and satellites: formation, stars: abundances, stars: atmospheres, open clusters and associations: individual: the Hyades},
	month = apr,
	pages = {3934-3948},
	primaryclass = {astro-ph.SR},
	title = {{The Hyades open cluster is chemically inhomogeneous}},
	volume = 457,
