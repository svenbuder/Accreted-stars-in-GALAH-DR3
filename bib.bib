	eprint = {1608.06390},
	journal = {\mnras},
	keywords = {radiative transfer, line: formation, stars: abundances, stars: atmospheres, methods: numerical},
	month = dec,
	pages = {1518-1533},
	primaryclass = {astro-ph.SR},
	title = {{Non-LTE line formation of Fe in late-type stars - III. 3D non-LTE analysis of metal-poor stars}},
