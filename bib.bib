	doi = {10.1093/mnras/stt533},
	eprint = {1303.6656},
	journal = {\mnras},
	keywords = {Galaxy: bulge, Galaxy: centre, Galaxy: evolution, Galaxy: formation, Galaxy: kinematics and dynamics},
	month = jul,
