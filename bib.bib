	journal = {\aaps},
	keywords = {GALAXY: EVOLUTION, STELLAR CONTENT, STARS: POPULATION II},
	month = dec,
	pages = {341-358},
	title = {{Uvby-{$\beta$} photometry of high-velocity and metal-poor stars. VII. Ages of halo and thick-disk field stars.}},
