	archiveprefix = {arXiv},
	author = {{Fu}, X. and {Bressan}, A. and {Marigo}, P. and {Girardi}, L. and {Montalb{\'a}n}, J. and {Chen}, Y. and {Nanni}, A.},
	date-added = {2018-03-08 14:00:23 +0000},
	date-modified = {2018-03-08 14:00:23 +0000},
	doi = {10.1093/mnras/sty235},
	eprint = {1801.07137},
	journal = {\mnras},
	keywords = {stars: evolution, Hertzsprung-Russell and colour-magnitude diagrams, stars: interiors, stars: low-mass},
	month = may,
	pages = {496-511},
	primaryclass = {astro-ph.SR},
	title = {{New PARSEC data base of {$\alpha$}-enhanced stellar evolutionary tracks and isochrones - I. Calibration with 47 Tuc (NGC 104) and the improvement on RGB bump}},
	volume = 476,
	year = 2018,
