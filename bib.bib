	journal = {\aap},
	keywords = {Galaxy: evolution, nuclear reactions, nucleosynthesis, abundances, stars: abundances, cosmic rays},
	month = jun,
	pages = {A67},
	title = {{Production and evolution of Li, Be, and B isotopes in the Galaxy}},
	volume = 542,
	year = 2012,
