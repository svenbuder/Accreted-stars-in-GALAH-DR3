	author = {{Villalobos}, {\'A}. and {Helmi}, A.},
	date-added = {2019-03-06 14:37:47 +1100},
	date-modified = {2019-03-06 14:37:47 +1100},
	doi = {10.1111/j.1365-2966.2008.13979.x},
	journal = {\mnras},
	keywords = {Galaxy: disc , Galaxy: formation , Galaxy: kinematics and dynamics , galaxies: formation , galaxies: kinematics and dynamics , galaxies: structure},
	month = dec,
	pages = {1806-1827},
	title = {{Simulations of minor mergers - I. General properties of thick discs}},
	volume = 391,
	year = 2008,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1111/j.1365-2966.2008.13979.x}}

@article{Quinn1993,
	abstract = {{The paper considers dynamical aspects of mergers between disks and small
satellite companions, with special attention given to: the
self-consistent response of disks to satellite mergers, which
generalizes an earlier investigation by Quinn {\amp} Goodman (1986); the
evolution of satellite orbits and debris; and the long-term structure
and kinematics of thickened disks as compared with observed properties
of disk galaxies. For satellites with masses in the range 0.04-0.2
M$_{disk}$ on initially circular inclined prograde orbits merging
with disks similar to the thin disk of the Galaxy, it is found that the
dynamical coupling between disks and satellites leads to orbital decay.
Disks respond to mergers by spreading both radially and vertically. The
radial heating is driven largely by the spiral response of the
dynamically cool disks to satellite perturbations.
}},
