	author = {{Fern{\'a}ndez-Alvar}, E. and {Fern{\'a}ndez-Trincado}, J.~G. and {Moreno}, E. and {Schuster}, W.~J. and {Carigi}, L. and {Recio-Blanco}, A. and {Beers}, T.~C. and {Chiappini}, C. and {Anders}, F. and {Santiago}, B.~X. and {Queiroz}, A.~B.~A. and {P{\'e}rez-Villegas}, {\'A}. and {Zamora}, O. and {Garc{\'{\i}}a-Hern{\'a}ndez}, D.~A.},
	date-added = {2018-07-20 09:39:51 +0000},
