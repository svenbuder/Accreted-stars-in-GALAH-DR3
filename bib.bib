	date-added = {2018-04-16 17:42:14 +0000},
	date-modified = {2018-04-16 17:42:14 +0000},
	doi = {10.1088/0004-637X/774/1/72},
	eid = {72},
	eprint = {1306.5788},
	journal = {\apj},
	keywords = {astrochemistry, dust, extinction, ISM: lines and bands, local insterstellar matter},
	month = sep,
	pages = {72},
	title = {{Properties of Diffuse Interstellar Bands at Different Physical Conditions of the Interstellar Medium}},
	volume = 774,
	year = 2013,
	Bdsk-Url-1 = {https://dx.doi.org/10.1088/0004-637X/774/1/72}}

