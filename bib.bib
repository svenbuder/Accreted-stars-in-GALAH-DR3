	keywords = {Diffusion, Euclidean Geometry, Fokker-Planck Equation, Orbits, Stellar Motions, Entropy, Hamiltonian Functions, Maxwell-Boltzmann Density Function, Perturbation Theory, Secular Variations, Solar Neighborhood},
	month = feb,
	pages = {597-627},
	title = {{The diffusion of stars through phase space}},
	volume = 230,
	year = 1988,
