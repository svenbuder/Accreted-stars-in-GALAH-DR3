@article{Arnould1975,
	abstract = {{Summary. In the framework of Arnett's (1969) model, the post-shock
explosive nucleosynthesis of hydrogen- rich layers is investigated with
particular emphasis on the thermonuclear production of 7Li and 11B.
Rather large ranges of peak temperatures and densities are shown to be
consistent with the production of 7Li or 11B by this mechanism.
Particularly in the 7Li case, the overproduction with respect to the
solar composition is often appreciable. As far as 11B is concerned, the
results depend rather sensitively on the choice of the explosive
expansion time scale parameters and on the uncertainties in the 11C(p,
y)12N rate. In fact, our study shows that the probability of a
substantial 11B production increases with decreasing explosion time
scale (for a given peak density) and decreasing rate of the proton
capture by 11C. It is also demonstrated that there exists a range of
temperature-density conditions (T0 4-6 x 108 0K; 1- t03g cm 3) for which
the enhanced production of both 7Li and 11B - sometimes together with
13C, 15N and 170 - occurs in relative concentrations consistent with
solar system abundances (Cameron, 1968, 1973). Key words: explosive
nucleosynthesis - light elements solar system abundances
}},
	adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	adsurl = {http://adsabs.harvard.edu/abs/1975A%26A....42...55A},
	author = {{Arnould}, M. and {Norgaard}, H.},
	date-added = {2019-04-15 12:40:43 +0200},
