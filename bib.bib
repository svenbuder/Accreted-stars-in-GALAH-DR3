	doi = {10.3847/2041-8205/826/2/L25},
	eid = {L25},
	eprint = {1603.06574},
	journal = {\apjl},
	keywords = {methods: data analysis, stars: abundances, stars: atmospheres, techniques: spectroscopic},
	month = aug,
