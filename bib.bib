	journal = {\mnras},
	keywords = {stars: fundamental parameters, Galaxy: evolution, Galaxy: formation, solar neighbourhood, Galaxy: stellar content},
	month = jan,
	pages = {2610-2621},
	title = {{On the local stellar populations}},
	volume = 464,
	year = 2017,
	Bdsk-File-1 = {},
