	author = {{Liu}, S. and {Du}, C. and {Newberg}, H.~J. and {Chen}, Y. and {Wu}, Z. and {Ma}, J. and {Zhou}, X. and {Cao}, Z. and {Hou}, Y. and {Wang}, Y. and {Zhang}, Y.},
