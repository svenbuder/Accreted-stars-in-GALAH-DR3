	eprint = {1501.01311},
	journal = {\mnras},
	keywords = {galaxies: evolution, galaxies: formation, galaxies: haloes, cosmology: theory},
	month = jun,
	pages = {1937-1961},
	title = {{The EAGLE simulations of galaxy formation: calibration of subgrid physics and model variations}},
	volume = 450,
	year = 2015,
	Bdsk-File-1 = {},
