	journal = {\aapr},
	keywords = {Galaxy: general, Globular Clusters, Halo, Stars: abundances, Hertzsprung{\ndash}Russell and C{\ndash}M diagrams, Hertzsprung-Russell and C-M diagrams},
	month = feb,
	pages = {50},
