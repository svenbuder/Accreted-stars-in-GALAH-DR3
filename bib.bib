	eprint = {1709.09182},
	journal = {\mnras},
	keywords = {methods: numerical, techniques: spectroscopic, surveys, stars: fundamental parameters, infrared: stars},
	month = apr,
	pages = {2978-2993},
	primaryclass = {astro-ph.IM},
	title = {{An application of deep learning in the analysis of stellar spectra}},
