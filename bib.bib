	archiveprefix = {arXiv},
	author = {{Feuillet}, D.~K. and {Bovy}, J. and {Holtzman}, J. and {Girardi}, L. and {MacDonald}, N. and {Majewski}, S.~R. and {Nidever}, D.~L.},
	date-added = {2018-01-17 09:57:10 +0000},
