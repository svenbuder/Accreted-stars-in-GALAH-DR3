	doi = {10.1093/mnras/stx3025},
	eprint = {1708.07834},
	journal = {\mnras},
	keywords = {galaxies: evolution, galaxies: kinematics and dynamics, galaxies: spiral, galaxies: structure},
	month = mar,
	pages = {3629-3639},
	title = {{Origin of chemically distinct discs in the Auriga cosmological simulations}},
	volume = 474,
	year = 2018,
