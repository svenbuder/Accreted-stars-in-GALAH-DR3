	adsurl = {http://adsabs.harvard.edu/abs/2018arXiv180700822K},
	archiveprefix = {arXiv},
	author = {{Kos}, J. and {de Silva}, G. and {Buder}, S. and {Bland-Hawthorn}, J. and {Asplund}, M. and {D'Orazi}, V. and {Duong}, L. and {Freeman}, K. and {Lewis}, G.~F. and {Lin}, J. and {Lind}, K. and {Martell}, S.~L. and {Schlesinger}, K.~J. and {Sharma}, S. and {Simpson}, J.~D. and {Zucker}, D.~B. and {Zwitter}, T. and {Bedding}, T.~R. and {{\v C}otar}, K. and {Horner}, J. and {Nordlander}, T. and {Stello}, D. and {Ting}, Y.-S. and {Traven}, G.},
