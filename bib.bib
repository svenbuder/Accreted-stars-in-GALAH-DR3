	adsurl = {http://adsabs.harvard.edu/abs/2014A%26A...568A..25N},
	archiveprefix = {arXiv},
	author = {{Nissen}, P.~E. and {Chen}, Y.~Q. and {Carigi}, L. and {Schuster}, W.~J. and {Zhao}, G.},
	date-added = {2018-01-18 17:30:01 +0000},
