	journal = {\apj},
	keywords = {line: formation, methods: data analysis, stars: abundances, stars: atmospheres, techniques: spectroscopic},
	month = jun,
	pages = {159},
	primaryclass = {astro-ph.SR},
	title = {{Measuring Oxygen Abundances from Stellar Spectra without Oxygen Lines}},
	volume = 860,
