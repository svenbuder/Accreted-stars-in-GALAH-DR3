	doi = {10.1093/mnras/stw1351},
	eprint = {1606.00833},
	journal = {\mnras},
	month = sep,
	pages = {2174-2191},
	primaryclass = {astro-ph.IM},
	title = {{A new algorithm for optimizing the wavelength coverage for spectroscopic studies: Spectral Wavelength Optimization Code (SWOC)}},
