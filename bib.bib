	author = {{De Silva}, G.~M. and {Freeman}, K.~C. and {Bland-Hawthorn}, J. and {Asplund}, M. and {Bessell}, M.~S.},
	date-added = {2019-03-05 15:50:43 +1100},
	date-modified = {2019-03-05 15:50:43 +1100},
	doi = {10.1086/510131},
	eprint = {astro-ph/0610041},
	journal = {\aj},
	keywords = {Galaxy: evolution, open clusters and associations: individual: HR 1614 moving group, stars: abundances},
	month = feb,
	pages = {694-704},
	title = {{Chemically Tagging the HR 1614 Moving Group}},
