	journal = {\mnras},
	keywords = {methods: observational: large-scale structure of Universe, methods: observational, large-scale structure of Universe},
	month = oct,
	pages = {1537-1549},
	title = {{Multi-object spectroscopy field configuration by simulated annealing}},
	volume = 371,
