	archiveprefix = {arXiv},
	author = {{Bensby}, T. and {Yee}, J.~C. and {Feltzing}, S. and {Johnson}, J.~A. and {Gould}, A. and {Cohen}, J.~G. and {Asplund}, M. and {Mel{\'e}ndez}, J. and {Lucatello}, S. and {Han}, C. and {Thompson}, I. and {Gal-Yam}, A. and {Udalski}, A. and {Bennett}, D.~P. and {Bond}, I.~A. and {Kohei}, W. and {Sumi}, T. and {Suzuki}, D. and {Suzuki}, K. and {Takino}, S. and {Tristram}, P. and {Yamai}, N. and {Yonehara}, A.},
	date-added = {2018-11-18 09:29:45 +0100},
	date-modified = {2018-11-18 09:29:45 +0100},
