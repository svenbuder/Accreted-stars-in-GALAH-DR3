	adsurl = {https://ui.adsabs.harvard.edu/abs/2019ApJ...883..177N},
	archiveprefix = {arXiv},
	author = {{Ness}, M.~K. and {Johnston}, K.~V. and {Blancato}, K. and {Rix}, H. -W. and {Beane}, A. and {Bird}, J.~C. and {Hawkins}, K.},
	date-added = {2020-04-29 18:21:29 +1000},
	date-modified = {2020-04-29 18:21:36 +1000},
	doi = {10.3847/1538-4357/ab3e3c},
	eid = {177},
	eprint = {1907.10606},
	journal = {\apj},
	keywords = {Milky Way disk, Milky Way evolution, Milky Way formation, Milky Way Galaxy, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies},
	month = oct,
	number = {2},
	pages = {177},
	primaryclass = {astro-ph.SR},
