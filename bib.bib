	journal = {Astronomische Nachrichten},
	keywords = {Galaxy: abundances, Galaxy: disk, Galaxy: evolution, Galaxy: formation, Galaxy: kinematics and dynamics},
	month = sep,
	pages = {944},
	title = {{Milky Way chemo-dynamics in the era of Gaia}},
	volume = 337,
