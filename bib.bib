	doi = {10.1086/148429},
	journal = {\apj},
	month = nov,
	pages = {1447},
	title = {{Stellar Evolution. II. The Evolution of a 3 M\_$\{$sun$\}$ Star from the Main Sequence Through Core Helium Burning.}},
