	doi = {10.3847/0004-637X/831/2/171},
	eid = {171},
	eprint = {1608.08634},
	journal = {\apj},
	keywords = {binaries: general, nuclear reactions, nucleosynthesis, abundances, stars: AGB and post-AGB, stars: carbon, stars: chemically peculiar},
	month = nov,
	pages = {171},
	primaryclass = {astro-ph.SR},
	title = {{The Intermediate Neutron-capture Process and Carbon-enhanced Metal-poor Stars}},
	volume = 831,
