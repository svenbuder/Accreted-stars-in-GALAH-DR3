@article{Karakas2012,
	abstract = {{We present updated calculations of stellar evolutionary sequences and
detailed nucleosynthesis predictions for the brightest asymptotic giant
branch (AGB) stars in the Galaxy with masses between 5 M
$_{&sun;}$ and 9 M $_{&sun;}$, with an initial metallicity
of Z = 0.02 ([Fe/H] = 0.14). In our previous studies we used the
