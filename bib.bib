	date-added = {2018-02-18 18:49:03 +0000},
	date-modified = {2018-02-18 18:49:03 +0000},
	doi = {10.1086/381701},
	eprint = {astro-ph/0312054},
	journal = {\apj},
	keywords = {Galaxy: Open Clusters and Associations: Individual: Name: Hyades, Stars: Abundances, Stars: Atmospheres},
	month = mar,
	pages = {697-707},
	title = {{Magnesium Isotope Ratios in Hyades Stars}},
