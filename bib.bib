	adsurl = {http://adsabs.harvard.edu/abs/2011ApJ...738..187L},
	archiveprefix = {arXiv},
	author = {{Lee}, Y.~S. and {Beers}, T.~C. and {An}, D. and {Ivezi{\'c}}, {\v Z}. and {Just}, A. and {Rockosi}, C.~M. and {Morrison}, H.~L. and {Johnson}, J.~A. and {Sch{\"o}nrich}, R. and {Bird}, J. and {Yanny}, B. and {Harding}, P. and {Rocha-Pinto}, H.~J.},
	date-added = {2018-04-10 05:14:23 +0000},
