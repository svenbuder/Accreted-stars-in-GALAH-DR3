@article{Kormendy2004,
	abstract = {{The Universe is in transition. At early times, galactic evolution was
dominated by hierarchical clustering and merging, processes that are
violent and rapid. In the far future, evolution will mostly be secular
the slow rearrangement of energy and mass that results from interactions
involving collective phenomena such as bars, oval disks, spiral
structure, and triaxial dark halos. Both processes are important now.
This review discusses internal secular evolution, concentrating on one
important consequence, the buildup of dense central components in disk
galaxies that look like classical, merger-built bulges but that were
made slowly out of disk gas. We call these pseudobulges.
}},
	adsnote = {Provided by the SAO/NASA Astrophysics Data System},
