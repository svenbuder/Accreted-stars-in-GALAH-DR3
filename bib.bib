@article{Gurtovenko2015,
	abstract = {{We have summed up our investigations performed in 1970--1993. The main
        task of this paper is clearly to show processes of formation of
        spectral lines as well as their distinction by validity and by
        location. For 503 photospheric lines of various chemical
