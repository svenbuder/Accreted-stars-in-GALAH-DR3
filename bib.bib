@article{Wallerstein1997,
	abstract = {{Forty years ago Burbidge, Burbidge, Fowler, and Hoyle combined what we
would now call fragmentary evidence from nuclear physics, stellar
evolution and the abundances of elements and isotopes in the solar
system as well as a few stars into a synthesis of remarkable ingenuity.
Their review provided a foundation for forty years of research in all of
the aspects of low energy nuclear experiments and theory, stellar
modeling over a wide range of mass and composition, and abundance
