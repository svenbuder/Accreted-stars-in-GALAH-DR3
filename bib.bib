	archiveprefix = {arXiv},
	author = {{Hansen}, C.~J. and {Primas}, F. and {Hartman}, H. and {Kratz}, K.-L. and {Wanajo}, S. and {Leibundgut}, B. and {Farouqi}, K. and {Hallmann}, O. and {Christlieb}, N. and {Nilsson}, H.},
	date-added = {2019-04-16 13:44:55 +0200},
	date-modified = {2019-04-16 13:44:55 +0200},
