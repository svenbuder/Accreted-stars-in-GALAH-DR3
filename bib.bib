	journal = {\aap},
	keywords = {INSTRUMENTATION: MISCELLANEOUS, SPACE VEHICLES: INSTRUMENTS, ASTROMETRY, GALAXY: GENERAL, TECHNIQUES: PHOTOMETRIC, TECHNIQUES: RADIAL VELOCITIES},
	month = apr,
