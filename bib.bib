	keywords = {Abundance, Line Spectra, Photosphere, Solar Spectra, Zinc, Atmospheric Models, Meteoritic Composition, Molecular Oscillations},
	month = apr,
	pages = {361-363},
	title = {{A reassessment of the zinc solar abundance}},
	volume = 84,
	year = 1980}

