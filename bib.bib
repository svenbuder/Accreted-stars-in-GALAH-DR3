	date-added = {2019-03-26 13:38:19 +0100},
	date-modified = {2019-03-26 13:38:19 +0100},
	doi = {10.1146/annurev-astro-082812-140938},
	eprint = {1303.1957},
	journal = {\araa},
	month = aug,
	pages = {353-392},
	primaryclass = {astro-ph.SR},
	title = {{Asteroseismology of Solar-Type and Red-Giant Stars}},
	volume = 51,
	year = 2013,
