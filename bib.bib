	archiveprefix = {arXiv},
	author = {{Bergemann}, M. and {Collet}, R. and {Sch{\"o}nrich}, R. and {Andrae}, R. and {Kovalev}, M. and {Ruchti}, G. and {Hansen}, C.~J. and {Magic}, Z.},
	date-added = {2019-01-29 10:35:47 +0100},
	date-modified = {2019-03-25 20:38:20 +0100},
	doi = {10.3847/1538-4357/aa88b5},
	eid = {16},
	eprint = {1612.07363},
	journal = {\apj},
	keywords = {Galaxy: abundances, Galaxy: evolution, Galaxy: kinematics and dynamics, radiative transfer, stars: abundances, stars: late-type},
	month = sep,
	pages = {16},
	primaryclass = {astro-ph.SR},
