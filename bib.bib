	date-modified = {2017-08-21 02:28:46 +0000},
	doi = {10.3847/1538-4357/834/1/27},
	eid = {27},
	eprint = {1608.04737},
	journal = {\apj},
	keywords = {galaxies: formation, galaxies: kinematics and dynamics, galaxies: structure, Galaxy: abundances, Galaxy: disk, Galaxy: evolution},
	month = jan,
	pages = {27},
	title = {{The Relationship between Mono-abundance and Mono-age Stellar Populations in the Milky Way Disk}},
	volume = 834,
	year = 2017,
