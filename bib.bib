	date-added = {2020-04-20 12:57:06 +1000},
	date-modified = {2020-11-04 14:28:34 +1100},
	doi = {10.1146/annurev-astro-091918-104509},
	eprint = {1811.08041},
	journal = {\araa},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies},
	month = aug,
	pages = {571-616},
	primaryclass = {astro-ph.SR},
	title = {{Accuracy and Precision of Industrial Stellar Abundances}},
	volume = {57},
	year = 2019,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1146/annurev-astro-091918-104509}}

