	adsurl = {http://adsabs.harvard.edu/abs/2008ApJ...673..864J},
	author = {{Juri{\'c}}, M. and {Ivezi{\'c}}, {\v Z}. and {Brooks}, A. and {Lupton}, R.~H. and {Schlegel}, D. and {Finkbeiner}, D. and {Padmanabhan}, N. and {Bond}, N. and {Sesar}, B. and {Rockosi}, C.~M. and {Knapp}, G.~R. and {Gunn}, J.~E. and {Sumi}, T. and {Schneider}, D.~P. and {Barentine}, J.~C. and {Brewington}, H.~J. and {Brinkmann}, J. and {Fukugita}, M. and {Harvanek}, M. and {Kleinman}, S.~J. and {Krzesinski}, J. and {Long}, D. and {Neilsen}, Jr., E.~H. and {Nitta}, A. and {Snedden}, S.~A. and {York}, D.~G.},
