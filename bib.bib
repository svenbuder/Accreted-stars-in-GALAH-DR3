	author = {{Huber}, D. and {Zinn}, J. and {Bojsen-Hansen}, M. and {Pinsonneault}, M. and {Sahlholdt}, C. and {Serenelli}, A. and {Silva Aguirre}, V. and {Stassun}, K. and {Stello}, D. and {Tayar}, J. and {Bastien}, F. and {Bedding}, T.~R. and {Buchhave}, L.~A. and {Chaplin}, W.~J. and {Davies}, G.~R. and {Garc{\'{\i}}a}, R.~A. and {Latham}, D.~W. and {Mathur}, S. and {Mosser}, B. and {Sharma}, S.},
	date-added = {2019-04-02 14:48:20 +0200},
