	doi = {10.3847/0004-637X/817/1/40},
	eid = {40},
	eprint = {1511.04088},
	journal = {\apj},
	keywords = {Galaxy: disk, Galaxy: evolution, stars: abundances, stars: fundamental parameters},
	month = jan,
