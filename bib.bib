	eprint = {1810.02829},
	journal = {\mnras},
	keywords = {stars: fundamental parameters, stars: late-type},
	month = jan,
	pages = {895-920},
	primaryclass = {astro-ph.SR},
	title = {{Benchmark ages for the Gaia benchmark stars}},
	volume = 482,
	year = 2019,
