	archiveprefix = {arXiv},
	author = {{{\"O}nehag}, A. and {Gustafsson}, B. and {Korn}, A.},
	date-added = {2017-08-31 09:38:51 +0000},
	date-modified = {2017-08-31 09:38:51 +0000},
	doi = {10.1051/0004-6361/201322663},
	eid = {A102},
	eprint = {1310.6297},
	journal = {\aap},
	keywords = {stars: atmospheres, stars: fundamental parameters, stars: abundances, techniques: spectroscopic, open clusters and associations: individual: M 67, methods: observational},
	month = feb,
	pages = {A102},
	primaryclass = {astro-ph.SR},
	title = {{Abundances and possible diffusion of elements in M 67 stars}},
	volume = 562,
