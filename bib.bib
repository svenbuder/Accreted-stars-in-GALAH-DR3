	month = dec,
	number = {3},
	pages = {4093-4110},
	primaryclass = {astro-ph.GA},
	title = {{Isochrone ages for {\ensuremath{\sim}}3 million stars with the second Gaia data release}},
	volume = {481},
	year = 2018,
