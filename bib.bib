	author = {{Martig}, M. and {Minchev}, I. and {Ness}, M. and {Fouesneau}, M. and {Rix}, H.-W.},
	date-added = {2018-02-22 13:07:24 +0000},
	date-modified = {2018-02-22 13:07:24 +0000},
	doi = {10.3847/0004-637X/831/2/139},
