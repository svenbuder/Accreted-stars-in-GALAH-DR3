	journal = {Frontiers in Astronomy and Space Sciences},
	pages = {3},
	title = {Scaling Relations for Solar-Like Oscillations: A Review},
	url = {https://www.frontiersin.org/article/10.3389/fspas.2020.00003},
	volume = {7},
	year = {2020},
	Bdsk-File-1 = {},
