	keywords = {methods: numerical , Galaxy: kinematics and dynamics , solar neighbourhood , Galaxy: structure},
	month = oct,
	pages = {429-437},
	title = {{Disassembling the Galaxy with angle-action coordinates}},
	volume = 390,
