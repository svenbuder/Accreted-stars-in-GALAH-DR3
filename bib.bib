	eprint = {0901.2743},
	journal = {\apjl},
	keywords = {Galaxy: abundances, Galaxy: disk, Galaxy: evolution, open clusters and associations: general},
	month = mar,
	pages = {L31-L34},
	primaryclass = {astro-ph.SR},
	title = {{Enhanced Production of Barium in Low-Mass Stars: Evidence from Open Clusters}},
