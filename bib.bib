	month = sep,
	pages = {1068-1080},
	primaryclass = {astro-ph.SR},
	title = {{PARSEC evolutionary tracks of massive stars up to 350 $\mathrm{M_\odot}$ at metallicities 0.0001 ${\leq}$ Z ${\leq}$ 0.04}},
