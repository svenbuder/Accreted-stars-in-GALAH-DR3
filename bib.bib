	eprint = {1511.08213},
	journal = {\mnras},
	keywords = {methods: numerical, galaxies: kinematics and dynamics},
	month = apr,
	pages = {2107-2121},
	title = {{A review of action estimation methods for galactic dynamics}},
	volume = 457,
	year = 2016,
