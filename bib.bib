	doi = {10.1093/mnras/sty709},
	journal = {\mnras},
	keywords = {stars: fundamental parameters},
	month = mar,
	title = {{Stellar ages and masses in the solar neighbourhood: Bayesian analysis using spectroscopy and Gaia DR1 parallaxes}},
	year = 2018,
	Bdsk-File-1 = {},
