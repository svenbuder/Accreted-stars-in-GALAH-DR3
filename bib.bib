	archiveprefix = {arXiv},
	author = {{Yong}, D. and {Casagrande}, L. and {Venn}, K.~A. and {Chen{\'e}}, A.-N. and {Keown}, J. and {Malo}, L. and {Martioli}, E. and {Alves-Brito}, A. and {Asplund}, M. and {Dotter}, A. and {Martell}, S.~L. and {Mel{\'e}ndez}, J. and {Schlesinger}, K.~J.},
	date-added = {2018-11-23 12:53:01 +0100},
	date-modified = {2018-11-23 12:53:01 +0100},
	doi = {10.1093/mnras/stw676},
	eprint = {1603.07034},
