	date-modified = {2020-08-17 17:50:22 +1000},
	doi = {10.1093/mnras/stw2759},
	eprint = {1608.00971},
	journal = {\mnras},
	keywords = {methods: statistical, Galaxy: fundamental parameters, Galaxy: kinematics and dynamics, Galaxy: structure, Astrophysics - Astrophysics of Galaxies},
	month = feb,
	number = {1},
	pages = {76-94},
	primaryclass = {astro-ph.GA},
	title = {{The mass distribution and gravitational potential of the Milky Way}},
