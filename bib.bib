	journal = {\mnras},
	keywords = {methods: data analysis, surveys, Galaxy: evolution, Galaxy: kinematics and dynamics},
	month = mar,
	pages = {294-304},
	title = {{MADE: a spectroscopic mass, age, and distance estimator for red giant stars with Bayesian machine learning}},
	volume = 484,
