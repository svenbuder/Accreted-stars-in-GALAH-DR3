	date-added = {2018-01-23 08:21:56 +0000},
	date-modified = {2018-01-23 08:21:56 +0000},
	doi = {10.1051/0004-6361/201730427},
	eid = {A75},
	eprint = {1708.01949},
	journal = {\aap},
	keywords = {stars: abundances, stars: atmospheres, techniques: spectroscopic, line: formation},
	month = nov,
	pages = {A75},
	primaryclass = {astro-ph.SR},
	title = {{Non-LTE aluminium abundances in late-type stars}},
