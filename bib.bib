	archiveprefix = {arXiv},
	author = {{Mackereth}, J.~T. and {Schiavon}, R.~P. and {Pfeffer}, J. and {Hayes}, C.~R. and {Bovy}, J. and {Anguiano}, B. and {Allende Prieto}, C. and {Hasselquist}, S. and {Holtzman}, J. and {Johnson}, J.~A. and {Majewski}, S.~R. and {O'Connell}, R. and {Shetrone}, M. and {Tissera}, P.~B. and {Fern{\'a}ndez-Trincado}, J.~G.},
