@article{Hawkins2017,
	abstract = {{Distances to individual stars in our own Galaxy are critical in order to
        piece together the nature of its velocity and spatial structure.
        Core helium burning red clump (RC) stars have similar
        luminosities, are abundant throughout the Galaxy and thus
        constitute good standard candles. We build a hierarchical
        probabilistic model to quantify the quality of RC stars as
        standard candles using parallax measurements from the first Gaia
        data release. A unique aspect of our methodology is to fully
        account for (and marginalize over) parallax, photometry and dust
        correction uncertainties, which lead to more robust results than
        standard approaches. We determine the absolute magnitude and
        intrinsic dispersion of the RC in 2MASS bands J, H, K$_{s}$,
        Gaia G band and WISE bands W1, W2, W3 and W4. We find that the
        absolute magnitude of the RC is -1.61 {\ensuremath{\pm}} 0.01
        (in K$_{s}$), +0.44 {\ensuremath{\pm}} 0.01 (in G), -0.93
        {\ensuremath{\pm}} 0.01 (in J), -1.46 {\ensuremath{\pm}} 0.01
        (in H), -1.68 {\ensuremath{\pm}} 0.02 (in W1), -1.69
        {\ensuremath{\pm}} 0.02 (in W2), -1.67 {\ensuremath{\pm}} 0.02
        (in W3) and -1.76 {\ensuremath{\pm}} 0.01 mag (in W4). The mean
        intrinsic dispersion is {\ensuremath{\sim}}0.17
        {\ensuremath{\pm}} 0.03 mag across all bands (yielding a typical
        distance precision of {\ensuremath{\sim}}8 per cent). Thus RC
        stars are reliable and precise standard candles. In addition, we
        have also re-calibrated the zero-point of the absolute magnitude
        of the RC in each band, which provides a benchmark for future
        studies to estimate distances to RC stars. Finally, the parallax
        error shrinkage in the hierarchical model outlined in this work
        can be used to obtain more precise parallaxes than Gaia for the
        most distant RC stars across the Galaxy.}},
	adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	adsurl = {https://ui.adsabs.harvard.edu/abs/2017MNRAS.471..722H},
	archiveprefix = {arXiv},
	author = {{Hawkins}, Keith and {Leistedt}, Boris and {Bovy}, Jo and {Hogg}, David W.},
	date-added = {2020-08-27 11:57:21 +1000},
	date-modified = {2020-08-27 11:57:25 +1000},
	doi = {10.1093/mnras/stx1655},
	eprint = {1705.08988},
	journal = {\mnras},
	keywords = {Stars: distances, Stars: fundamental parameters, Stars: statistics, Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
	month = oct,
	number = {1},
	pages = {722-729},
	primaryclass = {astro-ph.GA},
	title = {{Red clump stars and Gaia: calibration of the standard candle using a hierarchical probabilistic model}},
	volume = {471},
