	adsurl = {http://adsabs.harvard.edu/abs/2014AJ....148...81M},
	archiveprefix = {arXiv},
	author = {{Munari}, U. and {Henden}, A. and {Frigo}, A. and {Zwitter}, T. and {Bienaym{\'e}}, O. and {Bland-Hawthorn}, J. and {Boeche}, C. and {Freeman}, K.~C. and {Gibson}, B.~K. and {Gilmore}, G. and {Grebel}, E.~K. and {Helmi}, A. and {Kordopatis}, G. and {Levine}, S.~E. and {Navarro}, J.~F. and {Parker}, Q.~A. and {Reid}, W. and {Seabroke}, G.~M. and {Siebert}, A. and {Siviero}, A. and {Smith}, T.~C. and {Steinmetz}, M. and {Templeton}, M. and {Terrell}, D. and {Welch}, D.~L. and {Williams}, M. and {Wyse}, R.~F.~G.},
	date-added = {2018-04-04 23:22:33 +0000},
